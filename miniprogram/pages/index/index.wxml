<!--pages/index/index.wxml-->
<view class="container">
  <view class="input-wrap">
    <view>用户名：</view>
    <input value="{{username}}" bindinput="inputUsername" class="info-input"/>
  </view>
  <view class="inputwrap">
    <view>密码：</view>
    <input value="{{password}}" bindinput="inputPassword" class="info-input"/>
  </view>

  <button bindtap="login" class="login">登录</button>
  <button bindtap="register" class="login">注册</button>
</view>
